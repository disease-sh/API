{
	"extends": ["@aero", "prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly",
		"google": "readonly",
		"grecaptcha": "readonly"
	},
	"rules": {
		"no-unused-expressions": [
			1,
			{
				"allowTernary": true
			}
		]
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true
	}
}
